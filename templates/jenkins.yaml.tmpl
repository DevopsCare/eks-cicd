                - containerEnvVar:
                    key: "GIT_COMMITTER_NAME"
                    value: "cadmium"
                - containerEnvVar:
                    key: "XDG_CONFIG_HOME"
                    value: "/home/jenkins"
                label: "jenkins-kubernetes"
                name: "kubernetes"
                serviceAccount: "jenkins"
                slaveConnectTimeoutStr: "0"
                volumes:
                - hostPathVolume:
                    hostPath: "/var/run/docker.sock"
                    mountPath: "/var/run/docker.sock"
                - secretVolume:
